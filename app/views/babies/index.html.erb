<% provide(:title, "Your Babies") %>
<%= link_to "New Baby", new_baby_path(@baby), class: "btn pull-right" %>
<h2>Your Babies</h2>

<div class="row">
  <% if @babies.nil? %>
    <div class="span12">
      <p>You have no babies!</p>
    </div>
  <% else %>
    <%= render @babies %>
  <% end %>
</div>